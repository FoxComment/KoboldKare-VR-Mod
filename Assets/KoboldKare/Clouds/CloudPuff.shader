				float temp_output_2_0_g2 = _ForwardScattering;
				float temp_output_3_0_g2 = ( temp_output_2_0_g2 * temp_output_2_0_g2 );
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float3 lerpResult69 = lerp( _MainLightPosition.xyz , ( _WorldSpaceCameraPos - _MainLightPosition.xyz ) , _MainLightPosition.w);
				float dotResult14 = dot( ase_worldViewDir , lerpResult69 );
				float temp_output_1_0_g1 = dotResult14;
				float temp_output_2_0_g3 = -_BackScattering;
				float temp_output_3_0_g3 = ( temp_output_2_0_g3 * temp_output_2_0_g3 );
				float3 ase_worldNormal = IN.ase_texcoord3.xyz;
				float dotResult20 = dot( ase_worldViewDir , ase_worldNormal );
				float temp_output_22_0 = saturate( dotResult20 );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( _CloudColor * ( ( _Brightness + ( ( ( ( ( 1.0 - temp_output_3_0_g2 ) / ( ( 4.0 * PI ) * pow( ( ( temp_output_3_0_g2 + 1.0 ) - ( temp_output_2_0_g2 * 2.0 * temp_output_1_0_g1 ) ) , 1.5 ) ) ) * ( 1.0 - 0.5 ) ) + ( ( ( 1.0 - temp_output_3_0_g3 ) / ( ( 4.0 * PI ) * pow( ( ( temp_output_3_0_g3 + 1.0 ) - ( temp_output_2_0_g3 * 2.0 * temp_output_1_0_g1 ) ) , 1.5 ) ) ) * 0.5 ) ) * _PhaseFactor ) ) * ( temp_output_22_0 * temp_output_22_0 ) ) ).rgb;
